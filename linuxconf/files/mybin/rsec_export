#!/bin/bash

if [ $# -eq 0 ]; then
    echo "Usage: $0 RSEC_KEY1 RSEC_KEY2 genpasswd_google.com genpasswd_gpt.com ..."
    exit 1
fi

echo "# Generated by rsec_export. DO NOT modify"
for key in "$@"; do
    if [[ "$key" == genpasswd_* ]]; then
        arg="${key#genpasswd_}"
        value=$(genpasswd "$arg")
    else
        value=$(rsec "$key")
    fi
    echo "${key}:${value}"
done
